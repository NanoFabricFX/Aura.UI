	<Style Selector="NavigationView">
		<Setter Property="HorizontalAlignment" Value="Left"/>
		<Setter Property="ItemsPanel">
			<ItemsPanelTemplate>
				<StackPanel Orientation="Vertical"></StackPanel>
			</ItemsPanelTemplate>
		</Setter>
		<Setter Property="Template">
			<ControlTemplate>
				<Grid>
				<Border Name="PART_Border" BoxShadow="0 0 5 0 Black" HorizontalAlignment="Left">
					<Border.Transitions>
						<Transitions>
							<DoubleTransition  Property="Width"/>
						</Transitions>
					</Border.Transitions>
					<Grid>
					<Border Background="{DynamicResource ThemeAccentBrush2}" Opacity="0.5" Margin="0"/>
					<ExperimentalAcrylicBorder Material="{DynamicResource ThemeAcrylicBackgroundBrush}" 
										   HorizontalAlignment="Stretch">
						<Grid Name="PART_ItemsContainer" VerticalAlignment="Stretch" 
							  HorizontalAlignment="Stretch">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto"/>
								<RowDefinition Height="*"/>
								<RowDefinition Height="*"/>
							</Grid.RowDefinitions>
							<NavigationViewItem Name="PART_ToggleNav"
											VerticalAlignment="Top"
											Icon="{DynamicResource FluentIcons.navigation_regular}" 
											Header="{TemplateBinding Header}"
											HeaderTemplate="{TemplateBinding Header}"/>
							<ItemsPresenter Grid.Row="1"
										Name="PART_ItemsPresenter"
										Items="{TemplateBinding Items}"
										ItemsPanel="{TemplateBinding ItemsPanel}"/>
						</Grid>	
					</ExperimentalAcrylicBorder>
				</Grid>
				</Border>
				<ContentPresenter	Margin="60 30 30 30"
									ZIndex="-1"
									Classes="Container"
									Content="{TemplateBinding SelectedContent}"
									ContentTemplate="{TemplateBinding SelectedContentTemplate}"/>
				</Grid>						
			</ControlTemplate>   
		</Setter>
	</Style>
	<Style Selector="NavigationView /template/ NavigationViewItem#PART_ToggleNav /template/ ContentPresenter#PART_Presenter">
		<Setter Property="TextBlock.FontWeight" Value="ExtraBold"/>
	</Style>
	<Style Selector="NavigationView[ExpansionState=Hidden] /template/ Border#PART_Border">
		<Style.Animations>
			<Animation Duration="0:0:0.3" Easing="QuadraticEaseInOut" FillMode="Forward" >
				<KeyFrame Cue="0%">
					<Setter Property="Width" Value="{DynamicResource DefaultNavigationViewItemsContainerWidth}"/>
				</KeyFrame>
				<KeyFrame Cue="100%">
					<Setter Property="Width" Value="50"/>
				</KeyFrame>
			</Animation>
		</Style.Animations>
	</Style>
	<Style Selector="NavigationView[ExpansionState=Total] /template/ Border#PART_Border">
		<Style.Animations>
			<Animation Duration="0:0:0.3" Easing="QuadraticEaseOut" FillMode="Forward" >
				<KeyFrame Cue="0%">
					<Setter Property="Width" Value="50"/>
				</KeyFrame>
				<KeyFrame Cue="100%">
					<Setter Property="Width" Value="{DynamicResource DefaultNavigationViewItemsContainerWidth}"/>
				</KeyFrame>
			</Animation>
		</Style.Animations>
	</Style>
	<Style Selector="NavigationView[ExpansionState=Hidden] NavigationViewItem">
		<Setter Property="ExpansionState" Value="Hidden"/>
	</Style>
	<Style Selector="NavigationView[ExpansionState=Total] NavigationViewItem">
		<Setter Property="ExpansionState" Value="Total"/>
	</Style>
